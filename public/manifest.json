{
    "manifest_version": 3,
    "name": "sourcing-extension",
    "description": "automates sourcing for marketing professionals",
    "version": "1.0",
    "action": {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "serviceWorker.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["contentScript.js"]
        }
    ]
}
